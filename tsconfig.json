{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "es2019",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "importHelpers": true,
    "types": [
      "react/experimental",
      "react-dom/experimental"
    ],
    "lib": [
      "es2019",
      "dom"
    ]
  }
}
